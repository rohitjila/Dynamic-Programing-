class Solution:
    def cutRod(self, price, N):
        length = []
        for i in range(1,N+1):
            length.append(i)
        dp=[[0 for i in range(N+1)] for i in range(N+1)]
        for i in range(1,N+1):
            for j in range(1,N+1):
                if (length[i-1] <= j):
                    dp[i][j] = max(price[i-1] + dp[i][j-length[i-1]] , dp[i-1][j])
                else:
                    dp[i][j] = dp[i-1][j]
        return dp[N][N]
