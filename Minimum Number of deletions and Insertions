class Solution:
	def minOperations(self, X, Y):
		m=len(X)
		n=len(Y)
		dp=[[0 for i in range(m+1)] for i in range(n+1)]
        for i in range(1,n+1):
            for j in range(1,m+1):
                if (X[j-1] == Y[i-1]):
                    dp[i][j] = 1 + dp[i-1][j-1]
                else:
                    dp[i][j] = max(dp[i-1][j],dp[i][j-1])
        res1=m-dp[n][m]
        res2=n-dp[n][m]
        return res1+res2
        
